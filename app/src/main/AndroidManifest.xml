<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="analytics.client.capture.app.com">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <receiver android:name=".reciever.AnalyticsBrocastReceiver">
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED" />
            </intent-filter>
        </receiver>

        <service android:name=".workmanger.CaptureService"/>
        <receiver android:name=".reciever.UserSwitchReceiver">
            <intent-filter>
                <action android:name="android.intent.action.USER_BACKGROUND_EMULATED"/>
            </intent-filter>
        </receiver>
        <receiver android:name=".reciever.StateChangeRecevier">
            <intent-filter>
                <action android:name="harman.intent.action.SYSTEM_STATE_CHANGED"/>
                <category android:name="gm.category.VEHICLE_HMI" />

            </intent-filter>
        </receiver>
    </application>

</manifest>